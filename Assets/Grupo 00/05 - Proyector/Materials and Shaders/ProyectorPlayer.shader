			#else
				UNITY_GLOSSY_ENV_FROM_SURFACE( g, s, data );
				gi = UnityGlobalIllumination( data, s.Occlusion, s.Normal, g );
			#endif
		}

		void surf( Input i , inout SurfaceOutputStandardCustom o )
		{
			float4 color21 = IsGammaSpace() ? float4(1,0.08018869,0.9809323,0) : float4(1,0.007218568,0.9571708,0);
			float2 uv_TextureSample0 = i.uv_texcoord * _TextureSample0_ST.xy + _TextureSample0_ST.zw;
			float4 lerpResult4 = lerp( color21 , float4( 0,0,0,0 ) , tex2D( _TextureSample0, uv_TextureSample0 ).r);
			o.Translucency = ( lerpResult4 * 1.0 ).rgb;
			o.Alpha = 1;
		}

		ENDCG
